{
  "expo": {
    "name": "IRinger",
    "slug": "iringer_alpha",
    "description": "IRinger Project",
    "version": "0.0.0",
    "privacy": "public",
    "orientation": "portrait",
    "icon": "./assets/images/iRingerIcon_156.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
